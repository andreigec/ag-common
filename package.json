{
  "name": "ag-common",
  "version": "0.0.417",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Andrei Gec <@andreigec> (https://gec.dev/)",
  "repository": "github:andreigec/ag-common",
  "license": "ISC",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
